{
    "name": "website",
    "description": "website",
    "version": "0.0.1",
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev": "sapper dev --ext '.svelte .svexy'",
        "build": "sapper build --legacy --ext '.svelte .svexy'",
        "export": "NODE_ENV=production sapper export --legacy --ext '.svelte .svexy' --entry \"/ /sveltik/introducing\"",
        "now-build": "yarn run export && cp -r ./__sapper__/export ./public",
        "start": "node __sapper__/build",
        "cy:run": "cypress run",
        "cy:open": "cypress open",
        "test": "run-p --race dev cy:run",
        "format": "prettier --write \"**/*.js\" \"**/*.json\"",
        "lint": "eslint ."
    },
    "dependencies": {
        "compression": "^1.7.1",
        "polka": "next",
        "sirv": "^0.4.0"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/plugin-transform-runtime": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "@babel/runtime": "^7.0.0",
        "@fullhuman/postcss-purgecss": "^2.0.5",
        "@rollup/plugin-commonjs": "^11.0.0",
        "@rollup/plugin-node-resolve": "^7.0.0",
        "@rollup/plugin-replace": "^2.2.0",
        "autoprefixer": "^9.7.4",
        "cypress": "^3.8.3",
        "eslint": "^5.16.0 || ^6.7.2",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.20.1",
        "isomorphic-fetch": "^2.2.1",
        "less": "^3.11.1",
        "markdown-it-prism": "^2.0.5",
        "mdsvex": "^0.4.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^7.0.26",
        "postcss-load-config": "^2.1.0",
        "prettier": "^1.19.1",
        "rollup": "^1.20.0",
        "rollup-plugin-babel": "^4.0.2",
        "rollup-plugin-svelte": "^5.0.1",
        "rollup-plugin-terser": "^4.0.4",
        "sapper": "^0.27.10",
        "svelte": "^3.0.0",
        "svelte-preprocess": "^3.4.0",
        "sveltik": "^0.1.10",
        "tailwindcss": "^1.2.0"
    }
}
