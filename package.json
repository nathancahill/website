{
    "name": "website",
    "description": "website",
    "version": "0.0.1",
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev": "sapper dev --ext '.svelte .svexy'",
        "build": "sapper build --legacy --ext '.svelte .svexy'",
        "export": "NODE_ENV=production sapper export --legacy --ext '.svelte .svexy' --entry \"/ /sveltik/ /sveltik/introducing /dainte/ /dainte/introducing /vercel-sns-integration\"",
        "now-build": "yarn run export && cp -r ./__sapper__/export ./public",
        "start": "node __sapper__/build",
        "cy:run": "cypress run",
        "cy:open": "cypress open",
        "test": "start-server-and-test dev http://localhost:3000 cy:run",
        "format": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.svelte\" \"**/*.svexy\"",
        "lint": "eslint ."
    },
    "dependencies": {
        "compression": "^1.7.1",
        "polka": "next",
        "sirv": "^1.0.10"
    },
    "devDependencies": {
        "@babel/core": "^7.12.10",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/plugin-transform-runtime": "^7.12.10",
        "@babel/preset-env": "^7.12.11",
        "@babel/runtime": "^7.12.5",
        "@fullhuman/postcss-purgecss": "^3.0.0",
        "@rollup/plugin-babel": "^5.2.2",
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-node-resolve": "^11.0.1",
        "@rollup/plugin-replace": "^2.3.4",
        "@tailwindcss/ui": "^0.7.2",
        "autoprefixer": "^10.0.0",
        "cypress": "^6.2.1",
        "eslint": "^7.17.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^7.1.0",
        "eslint-plugin-import": "^2.22.1",
        "isomorphic-fetch": "^3.0.0",
        "less": "^4.1.0",
        "markdown-it-prism": "^2.1.4",
        "mdsvex": "^0.8.9",
        "postcss": "^8.0.9",
        "postcss-load-config": "^3.0.0",
        "prettier": "^2.2.1",
        "prettier-plugin-svelte": "^2.0.3",
        "rollup": "^2.36.1",
        "rollup-plugin-svelte": "^6.1.1",
        "rollup-plugin-terser": "^7.0.2",
        "sapper": "^0.28.10",
        "start-server-and-test": "^1.11.7",
        "svelte": "^3.31.2",
        "svelte-preprocess": "^4.6.1",
        "sveltik": "^0.1.16",
        "tailwindcss": "^1.9.6"
    }
}
