{
    "name": "website",
    "description": "website",
    "version": "0.0.1",
    "license": "MIT",
    "private": true,
    "scripts": {
        "dev": "sapper dev --ext '.svelte .svexy'",
        "build": "sapper build --legacy --ext '.svelte .svexy'",
        "export": "NODE_ENV=production sapper export --legacy --ext '.svelte .svexy' --entry \"/ /sveltik/ /sveltik/introducing /dainte/ /dainte/introducing /vercel-sns-integration\"",
        "now-build": "yarn run export && cp -r ./__sapper__/export ./public",
        "start": "node __sapper__/build",
        "cy:run": "cypress run",
        "cy:open": "cypress open",
        "test": "start-server-and-test dev http://localhost:3000 cy:run",
        "format": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.svelte\" \"**/*.svexy\"",
        "lint": "eslint ."
    },
    "dependencies": {
        "compression": "^1.7.1",
        "polka": "next",
        "sirv": "^1.0.6"
    },
    "devDependencies": {
        "@babel/core": "^7.11.6",
        "@babel/plugin-syntax-dynamic-import": "^7.0.0",
        "@babel/plugin-transform-runtime": "^7.11.5",
        "@babel/preset-env": "^7.11.5",
        "@babel/runtime": "^7.11.2",
        "@fullhuman/postcss-purgecss": "^3.0.0",
        "@rollup/plugin-babel": "^5.2.1",
        "@rollup/plugin-commonjs": "^15.1.0",
        "@rollup/plugin-node-resolve": "^9.0.0",
        "@rollup/plugin-replace": "^2.3.3",
        "@tailwindcss/ui": "^0.6.2",
        "autoprefixer": "^10.0.0",
        "cypress": "^5.3.0",
        "eslint": "^7.11.0",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-prettier": "^6.12.0",
        "eslint-plugin-import": "^2.22.1",
        "isomorphic-fetch": "^3.0.0",
        "less": "^3.12.2",
        "markdown-it-prism": "^2.1.3",
        "mdsvex": "^0.8.8",
        "postcss": "^8.0.9",
        "postcss-load-config": "^3.0.0",
        "prettier": "^2.1.2",
        "prettier-plugin-svelte": "^1.4.0",
        "rollup": "^2.29.0",
        "rollup-plugin-svelte": "^6.0.1",
        "rollup-plugin-terser": "^7.0.2",
        "sapper": "^0.28.10",
        "start-server-and-test": "^1.11.5",
        "svelte": "^3.29.0",
        "svelte-preprocess": "^4.5.1",
        "sveltik": "^0.1.13",
        "tailwindcss": "^1.9.1"
    }
}
